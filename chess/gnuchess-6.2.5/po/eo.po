#, c-format
msgid "My rating = %d\n"
msgstr "Mia pritakso = %d\n"

#: src/frontend/cmd.cc:927
#, c-format
msgid "Opponent rating = %d\n"
msgstr "Kontraŭula pritakso = %d\n"

#: src/frontend/cmd.cc:929
